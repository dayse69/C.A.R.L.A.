{
    "compilerOptions": {
        "target": "ES2022",
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "rootDir": "src",
        "outDir": "build",
        
        "paths": {
            "#env": ["./src/env/index.ts"],
            "#base": ["./src/discord/base/index.ts"]
        },
        "strict": false,
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "include": ["src"]
}
